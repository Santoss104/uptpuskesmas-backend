{
  "name": "apifrompuskesmas",
  "version": "1.0.0",
  "description": "selamat datang di API puskesmas",
  "main": "server.ts",
  "type": "module",
  "scripts": {
    "start": "node --loader tsx/esm server.ts",
    "start:js": "node build/server.js",
    "start:tsx": "tsx server.ts",
    "dev": "tsx --watch server.ts",
    "start:prod": "cross-env NODE_ENV=production node --loader tsx/esm server.ts",
    "start:dev": "cross-env NODE_ENV=development tsx server.ts",
    "start:prod:win": "set NODE_ENV=production && node --loader tsx/esm server.ts",
    "start:dev:win": "set NODE_ENV=development && tsx server.ts",
    "build": "tsc",
    "postbuild": "echo Build completed"
  },
  "author": "Ikhlas Abdillah",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.23",
    "@types/joi": "^17.2.2",
    "@types/multer": "^2.0.0",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.7.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.4",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "redis": "^5.6.0",
    "winston": "^3.17.0",
    "xlsx": "^0.18.5",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongoose": "^5.11.96",
    "@types/morgan": "^1.9.10",
    "@types/xlsx": "^0.0.35",
    "cross-env": "^10.0.0",
    "typescript": "^5.8.3"
  }
}
